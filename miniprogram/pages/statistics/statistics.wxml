<!--record/statistics/statistics.wxml-->
<view class="statistics container">
    <dateNav bind:updateList='getList' showMoney="{{false}}"></dateNav>
    <typePercent all="{{all}}" list="{{percentlist}}"></typePercent>
    <view>
        <ec-canvas id="mychart-dom-bar" canvas-id="mychart-bar" ec="{{ ec }}"></ec-canvas>
    </view>
</view>
